---
whiskers:
  version: "^2.5.1"
  matrix:
    - flavor
  filename: "src/main/resources/themes/{{ flavor.identifier }}.theme.json"
  hex_format: "#{{r}}{{g}}{{b}}{{z}}"
accent: "mauve"
secondary_accent: "yellow"
---

{%- macro file_color(color) -%}
  {{ if(cond = flavor.light, t = color | mix(color = base, amount = 0.15), f = color | mix(color = base, amount = 0.2)) | get(key = "hex") }}
{%- endmacro file_color -%}

{% set name = "Catppuccin " ~ flavor.name -%}
{% set editor_scheme = "/themes/" ~ flavor.identifier ~ ".xml" -%}
{% set primary_foreground = text -%}
{% set primary_background = base -%}
{% set secondary_background = surface0 -%}
{% set panel_background = mantle -%}
{% set hover_background = surface0 -%}
{% set selection_background = surface0 -%}
{% set selection_inactive_background = base -%}
{% set border_color = if(cond = flavor.dark, t = base, f = crust) -%}
{% set separator_color = surface0 -%}
{% set error_background = red | mix(color = base, amount = 0.1) -%}
{% set error_border_color = red | mix(color = base, amount = 0.15) -%}
{% set informative_background = blue | mix(color = base, amount = 0.1) -%}
{% set informative_border_color = blue | mix(color = base, amount = 0.15) -%}
{% set warning_background = peach | mix(color = base, amount = 0.1) -%}
{% set warning_border_color = peach | mix(color = base, amount = 0.15) -%}
{% set list_selection_inactive_background = if(cond = flavor.light, t = hover_background, f = base) -%}
{% set main_tab_foreground = primary_foreground | mix(color = base, amount = 0.5) -%}
{% set main_tab_hover_background = panel_background | mix(color = base, amount = 0.7) -%}
{% set search_match_background = if(cond = flavor.light, t = mauve | mix(color = base, amount = 0.5), f = mauve) -%}
{% set text_field_selection_background = if(cond = flavor.light, t = surface0 | mix(color = base, amount = 0.4), f = surface2) -%}
{% set commit_current_branch_background = mauve | mix(color = mantle, amount = 0.05) -%}

{
  "name": "{{ name }}",
  "dark": "{{ flavor.dark }}",
  "author": "Catppuccin Org <releases@catppuccin.com>",
  "editorScheme": "{{ editor_scheme }}",
  "colors": {
    "rosewater": "{{ rosewater.hex }}",
    "flamingo": "{{ flamingo.hex }}",
    "mauve": "{{ mauve.hex }}",
    "pink": "{{ pink.hex }}",
    "maroon": "{{ maroon.hex }}",
    "red": "{{ red.hex }}",
    "peach": "{{ peach.hex }}",
    "yellow": "{{ yellow.hex }}",
    "green": "{{ green.hex }}",
    "teal": "{{ teal.hex }}",
    "blue": "{{ blue.hex }}",
    "sky": "{{ sky.hex }}",
    "sapphire": "{{ sapphire.hex }}",
    "lavender": "{{ lavender.hex }}",
    "crust": "{{ crust.hex }}",
    "mantle": "{{ mantle.hex }}",
    "base": "{{ base.hex }}",
    "surface0": "{{ surface0.hex }}",
    "surface1": "{{ surface1.hex }}",
    "surface2": "{{ surface2.hex }}",
    "overlay0": "{{ overlay0.hex }}",
    "overlay1": "{{ overlay1.hex }}",
    "overlay2": "{{ overlay2.hex }}",
    "text": "{{ text.hex }}",
    "subtext0": "{{ subtext0.hex }}",
    "subtext1": "{{ subtext1.hex }}",
    "accentColor": "{{ flavor.colors[accent].hex }}",
    "secondaryAccentColor": "{{ flavor.colors[secondary_accent].hex }}",
    "primaryForeground": "{{ primary_foreground.hex }}",
    "primaryBackground": "{{ primary_background.hex }}",
    "secondaryBackground": "{{ secondary_background.hex }}",
    "panelBackground": "{{ panel_background.hex }}",
    "hoverBackground": "{{ hover_background.hex }}",
    "selectionBackground": "{{ selection_background.hex }}",
    "selectionInactiveBackground": "{{ selection_inactive_background.hex }}",
    "borderColor": "{{ border_color.hex }}",
    "separatorColor": "{{ separator_color.hex }}"
  },
  "ui": {
    "*": {
      "arc": "7",
      "background": "primaryBackground",
      "borderColor": "borderColor",
      "disabledBackground": "primaryBackground",
      "foreground": "primaryForeground",
      "inactiveBackground": "primaryBackground",
      "selectionBackground": "selectionBackground",
      "selectionForeground": "primaryForeground",
      "selectionInactiveBackground": "selectionInactiveBackground",
      "selectionInactiveForeground": "primaryForeground",
      "separatorColor": "separatorColor"
    },
    "ActionButton": {
