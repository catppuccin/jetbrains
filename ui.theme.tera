      "hoverBorderColor": "hoverBackground",
      "pressedBackground": "selectionBackground",
      "pressedBorderColor": "selectionBackground"
    },
    "Banner": {
      "errorBackground": "{{ error_background.hex }}",
      "errorBorderColor": "{{ error_border_color.hex }}",
      "errorForeground": "primaryForeground",
      "informativeBackground": "{{ informative_background.hex }}",
      "informativeBorderColor": "{{ informative_border_color.hex }}",
      "informativeForeground": "primaryForeground",
      "warningBackground": "{{ warning_background.hex }}",
      "warningBorderColor": "{{ warning_border_color.hex }}",
      "warningForeground": "primaryForeground"
